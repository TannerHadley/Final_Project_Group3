<h2>All Users</h2>
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Role</th>
            <th>Created At</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.password %></td>
                <td><%= user.role %></td>
            </tr>
        <% }) %>
    </tbody>
</table>
